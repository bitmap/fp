{
  "name": "@bitmap/fp",
  "version": "0.7.0",
  "description": "Function programming utilities",
  "license": "ISC",
  "author": "Cabe Branson <cabe@bitmap.dev> (https://bitmap.dev)",
  "repository": "github:bitmap/fp",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint lib",
    "dist": "rollup --config",
    "prepublishOnly": "npm run dist",
    "preversion": "npm test && npm run dist",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@cabe/eslint-config": "^1.0.6",
    "babel-jest": "^26.6.3",
    "benchmark": "^2.1.4",
    "eslint": "^7.13.0",
    "eslint-plugin-jest": "^24.1.3",
    "esm": "^3.2.25",
    "jest": "^26.6.3",
    "rollup": "^2.33.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "tmp"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "jest/globals": true
    },
    "extends": [
      "@cabe",
      "plugin:jest/recommended",
      "plugin:jest/style"
    ],
    "plugins": [
      "jest"
    ]
  },
  "eslintIgnore": [
    "dist"
  ]
}
