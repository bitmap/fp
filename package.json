{
  "name": "@bitmap/fp",
  "version": "0.8.0-1",
  "description": "Functional programming utilities",
  "license": "ISC",
  "author": "Cabe Branson <cabe@bitmap.dev> (https://bitmap.dev)",
  "repository": "github:bitmap/fp",
  "type": "module",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "preversion": "npm test"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@cabe/eslint-config": "^1.0.6",
    "babel-jest": "^26.6.3",
    "eslint": "^7.17.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "tmp"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "jest/globals": true
    },
    "extends": [
      "@cabe",
      "plugin:jest/recommended",
      "plugin:jest/style"
    ],
    "plugins": [
      "jest"
    ]
  },
  "eslintIgnore": [
    "dist"
  ]
}
