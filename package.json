    "rollup": "^4.18.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1",
    "vitest": "^1.6.0"
