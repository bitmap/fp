    "preversion": "npm test && npm run dist",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@cabe/eslint-config": "^1.0.6",
    "babel-jest": "^26.3.0",
    "benchmark": "^2.1.4",
    "eslint": "^7.9.0",
    "eslint-plugin-jest": "^24.0.1",
    "esm": "^3.2.25",
    "jest": "^26.4.2",
    "rollup": "^2.27.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "tmp"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "jest/globals": true
    },
    "extends": [
      "@cabe",
      "plugin:jest/all"
    ],
    "plugins": [
      "jest"
    ]
  },
  "eslintIgnore": [
    "dist"
  ]
